<div id="left" style="margin-top:18px;background:#fff;"> 
  <% unless @readonlypost1 %>
    <% image_tag "group.png" %>      
    <% link_to_remote ("My Contests (#{@user.posts.size})" + image_tag("bullet_arrow_down.png")), :url => { :action => :load_all_participants, :uid => @user.unique_id, :pid => @post.unique_id },:update => 'list-all-p' %>
           
       <ul id="participants-list1">
            <% unless @user.posts.empty? %>
                <% @user.posts.each do |post| %>
                    <% link_to  h(post.subject), post.get_url_for(@user, 'show'), :class => "#{post.unread_comments_for(@user) > 0 ? "unread-post" : "read-post"}" %>
                <% end %>
            <% end %>
       </ul>
      
      <div id="list-all-p11"></div>
 
    <% @number_of_matches = @contest.matches.size %>
    <% if @number_of_matches > 5 %>
        <div id="participants" style="height:662px;overflow:auto;">
    <% else %>
        <div id="participants">
    <% end %>     
        <div class="tc-row1" style="font-weight:bold;color:#21519C;height1:50px;border-bottom:1px solid #709CB2;"><% @post.subject %>
            <%= link_to "#{@number_of_matches} matches", @contest %>
        </div>
        <%= render :partial => 'match', :collection => @contest.matches %>      
    </div>
  <% end %>
</div>

