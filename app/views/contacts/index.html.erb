<h2 style="margin-left:10px;">My Contacts (<%= @contacts.count %>)</h2>
<fieldset>
     <% @contacts.each do |contact| %>        
          <div class="contact-name"><%= link_to contact.display_name, :controller => 'users', :action => 'display_profile', :id => contact %></div>
          <div class="contact-info"><%= "email: " + contact.email unless contact.email.include?("nonmember") %></div>
          <div class="contact-info1"><% "twitter: @" + contact.screen_name unless contact.screen_name.blank? %></div>
          <div class="contact-info1"><% "Tags: " + contact.tag_list unless contact.tag_list.blank? %></div>
        <p></p>              
    <% end %>
</fieldset>
